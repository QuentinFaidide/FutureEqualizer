@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.

<http://future-plugin-suite.org/lv2/future/equalizer/gui>
  a guiext:GtkUI;
  guiext:binary <FutureEqualizerGUI.so>;
  guiext:optionalFeature guiext:noUserResize ;
  lv2:optionalFeature guiext:noUserResize ;
  guiext:requiredFeature guiext:makeResident.

<http://future-plugin-suite.org/lv2/future/equalizer>
  a lv2:Plugin;
  lv2:binary <FutureEqualizer.so>;
  doap:name "Future Equalizer";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  guiext:ui <http://future-plugin-suite.org/lv2/future/equalizer/gui>;

  lv2:port [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "lcfreq" ;
    lv2:name "LowCut Frequency" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 10.00 ;
    lv2:minimum 10.0000 ;
    lv2:maximum 10000.0 ;
    ue:unit ue:hz ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "hcfreq" ;
    lv2:name "HighCut Frequency" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 20000.00 ;
    lv2:minimum 10.0000 ;
    lv2:maximum 20000.0 ;
    ue:unit ue:hz ;
  ],

  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "left_input";
    lv2:name "Left input";
  ],

  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "right_input";
    lv2:name "Right input";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 4;
    lv2:symbol "left_output";
    lv2:name "Left output";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 5;
    lv2:symbol "right_output";
    lv2:name "Right output";
  ],

  [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 6 ;
    lv2:symbol "lowcut_switch" ;
    lv2:name "LowCut Switch" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty lv2:toggled ;
    lv2:default 0.00000 ;
    lv2:minimum 0.00000 ;
    lv2:maximum 1.00000 ;
  ],

  [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 7 ;
    lv2:symbol "highcut_switch" ;
    lv2:name "HighCut Switch" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty lv2:toggled ;
    lv2:default 0.00000 ;
    lv2:minimum 0.00000 ;
    lv2:maximum 1.00000 ;
  ],

  [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 8 ;
    lv2:symbol "peak1_switch" ;
    lv2:name "Peak 1 Switch" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty lv2:toggled ;
    lv2:default 0.00000 ;
    lv2:minimum 0.00000 ;
    lv2:maximum 1.00000 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:symbol "peak1freq" ;
    lv2:name "Peak 1 Frequency" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 200.00 ;
    lv2:minimum 10.0000 ;
    lv2:maximum 20000.0 ;
    ue:unit ue:hz ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 10;
    lv2:symbol "peak1res" ;
    lv2:name "Peak 1 Resonance" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:default 1.0 ;
    lv2:minimum 0.600 ;
    lv2:maximum 10.0 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 11;
    lv2:symbol "peak1gain" ;
    lv2:name "Peak 1 Gain" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 0.0 ;
    lv2:minimum -24.0 ;
    lv2:maximum 24.0 ;
  ],

  [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 12 ;
    lv2:symbol "peak2_switch" ;
    lv2:name "Peak 2 Switch" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty lv2:toggled ;
    lv2:default 0.00000 ;
    lv2:minimum 0.00000 ;
    lv2:maximum 1.00000 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 13;
    lv2:symbol "peak2freq" ;
    lv2:name "Peak 2 Frequency" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 700.00 ;
    lv2:minimum 10.0000 ;
    lv2:maximum 20000.0 ;
    ue:unit ue:hz ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 14;
    lv2:symbol "peak2res" ;
    lv2:name "Peak 2 Resonance" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:default 1.0 ;
    lv2:minimum 0.600 ;
    lv2:maximum 10.0 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 15;
    lv2:symbol "peak2gain" ;
    lv2:name "Peak 2 Gain" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 0.0 ;
    lv2:minimum -24.0 ;
    lv2:maximum 24.0 ;
  ],

  [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 16 ;
    lv2:symbol "peak3_switch" ;
    lv2:name "Peak 3 Switch" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty lv2:toggled ;
    lv2:default 0.00000 ;
    lv2:minimum 0.00000 ;
    lv2:maximum 1.00000 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 17;
    lv2:symbol "peak3freq" ;
    lv2:name "Peak 3 Frequency" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 700.00 ;
    lv2:minimum 10.0000 ;
    lv2:maximum 20000.0 ;
    ue:unit ue:hz ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 18;
    lv2:symbol "peak3res" ;
    lv2:name "Peak 3 Resonance" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:default 1.0 ;
    lv2:minimum 0.600 ;
    lv2:maximum 10.0 ;
  ],

  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 19;
    lv2:symbol "peak3gain" ;
    lv2:name "Peak 3 Gain" ;
    lv2:portProperty epp:hasStrictBounds ;
    lv2:portProperty epp:logarithmic ;
    lv2:default 0.0 ;
    lv2:minimum -24.0 ;
    lv2:maximum 24.0 ;
  ].
